  "main": "dist/CETEI.js",
  "jsnext:main": "src/CETEI.js",
  "keywords": [
    "TEI",
    "XML",
    "HTML5 custom elements",
    "ODD"
  ],
  "author": "Raff Viglianti and Hugh Cayless",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TEIC/CETEIcean.git"
  },
  "devDependencies": {
    "babel-preset-es2015": "6.13.2",
    "babel-preset-es2015-rollup": "1.1.0",
    "http-server": "0.9.0",
    "onchange": "2.4.0",
    "rollup": "0.26.2",
    "rollup-plugin-babel": "2.4.0",
    "rollup-plugin-uglify": "1.0.0"
  },
  "scripts": {
